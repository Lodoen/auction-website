{
  "name": "auction-website",
  "version": "0.14.2",
  "description": "Auction website for electronics",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js & eslint src/**/*.mjs",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaslod/auction-website.git"
  },
  "keywords": [
    "auction",
    "website",
    "bootstrap",
    "api",
    "electronics"
  ],
  "author": "jonaslod",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonaslod/auction-website/issues"
  },
  "homepage": "https://github.com/jonaslod/auction-website#readme",
  "dependencies": {
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.3",
    "sass": "^1.58.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.mjs": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
